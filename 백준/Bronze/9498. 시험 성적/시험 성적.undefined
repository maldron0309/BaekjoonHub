#include <iostream>

int main() {
	int grade;

	std::cin >> grade;
    
    if (grade >= 90) std::cout << "A\n";
    else if (grade >=  80) std::cout << "B\n";
    else if (grade >= 70) std::cout << "C\n";
    else if (grade >= 60) std::cout << "D\n";
    else std::cout << "F";

	return 0;

}

